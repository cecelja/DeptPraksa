import java.util.ArrayList;
import java.util.List;

public class Task1 {
	
	public static void main(String[] args) {
		//Input data
		int[] niz = new int[]{1,3,1,7,5,6,6,7,4,2};
		
		//Calculate the total sum of the array
		int zbroj = 0;
		for (int i = 0; i < niz.length; i++) {
			zbroj += niz[i];
		}
		
		
		//Check if the total sum is divisible by 3
		if (zbroj % 3 == 0) {
			//The sum we wish to achieve with given elements
			int bound = zbroj / 3;
			
			int currentSum = 0;
			for (int j = 0; j < niz.length; j++) {
				if (currentSum <= bound) {
					currentSum += niz[j];
				} else if (currentSum == bound) {
					
				}
			}
		}	
	}

}
