package hr.filip.cecelja.interview;

public class Task2 {
	
	public static void main(String[] args) {
		
		int length  = 5;
		int n2 = 1;
		int n3 = length;
		
		for(int i = 0; i < length; i++) {
			for (int j = 0; j < n2; j++) {
				if (n3 == length) {
					System.out.print(n3);
				} else System.out.print("%d,", n3);
			}
			System.out.println();
			n2++;
			n3--;
		}
		
	}

}
